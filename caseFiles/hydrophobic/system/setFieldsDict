FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    // Set cell values
    // (does zerogradient on boundaries)
    sphereToCell
    {
        origin (0.0 0.0 0.0);
	    radius 0.01;
        centre (0.05 0.05 0.05);
        fieldValues ( volScalarFieldValue alpha.water 1 );
    }
);  
// ************************************************************************* //
